        <RadioButton x:Name="radioButton6" Content="6" HorizontalAlignment="Left" Margin="316,245,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButton7" Content="7" HorizontalAlignment="Left" Margin="316,265,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="How Many Drawers Would You Like Your Desk to Have?" HorizontalAlignment="Left" Margin="199,31,0,0" VerticalAlignment="Top"/>
