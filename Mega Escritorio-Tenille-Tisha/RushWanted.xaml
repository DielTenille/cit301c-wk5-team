<Page x:Class="Mega_Escritorio_Tenille_Tisha.RushWanted"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="RushWanted"
      Background="WhiteSmoke">

    <Grid>
        <Button x:Name="button" Content="Continue" HorizontalAlignment="Left" Margin="288,557,0,0" VerticalAlignment="Top" Width="170" Click="button_Click" FontSize="16" Height="50" RenderTransformOrigin="0.478,0.456" Background="Honeydew" BorderThickness="5"/>
        <RadioButton x:Name="radioButton" Content="Yes" HorizontalAlignment="Left" Margin="264,225,0,0" VerticalAlignment="Top" FontSize="16" Click="rushWanted_Loaded"/>
        <RadioButton x:Name="radioButton1" Content="No" HorizontalAlignment="Left" Margin="361,225,0,0" VerticalAlignment="Top" FontSize="16" Click="rushWanted_Loaded"/>
        <TextBox x:Name="label" Text="Do You Want Rush Order Shipping?" TextWrapping="Wrap" TextAlignment="Center" IsReadOnly="True" HorizontalAlignment="Left" Margin="0,90,0,0" VerticalAlignment="Top" Height="39" Width="700" FontSize="24" FontWeight="Bold" BorderThickness="0" Background="Transparent"/>
    </Grid>
</Page>
