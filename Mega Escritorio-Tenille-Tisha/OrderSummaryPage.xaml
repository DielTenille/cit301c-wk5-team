<Page x:Class="Mega_Escritorio_Tenille_Tisha.OrderSummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="Order Summary">

    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="220" Margin="42,48,0,0" VerticalAlignment="Top" Width="241"/>
        <Label x:Name="label" Content="Your Custom Desk Quote" HorizontalAlignment="Left" Margin="68,17,0,0" VerticalAlignment="Top" Width="178"/>
        <Button x:Name="button" Content="Start Over" HorizontalAlignment="Left" Margin="42,273,0,0" VerticalAlignment="Top" Width="75" Click="startOver"/>
        <Button x:Name="button1" Content="Order Now!" HorizontalAlignment="Left" Margin="223,273,0,0" VerticalAlignment="Top" Width="75" Click="completeOrder"/>

    </Grid>
</Page>
