<Page x:Class="Mega_Escritorio_Tenille_Tisha.OrderSummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="Order Summary"
      Background="WhiteSmoke">

    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="400" Margin="171,131,0,0" VerticalAlignment="Top" Width="360" BorderThickness="8" Background="White"/>
        <TextBox x:Name="label" Text="Your Custom Desk Quote" TextWrapping="Wrap" TextAlignment="Center" IsReadOnly="True" HorizontalAlignment="Left" Margin="0,73,0,0" VerticalAlignment="Top" Height="37" Width="700" FontSize="24" FontWeight="Bold" Background="Transparent" BorderThickness="0"/>
        <Button x:Name="button" Content="Start Over" HorizontalAlignment="Left" Margin="171,596,0,0" Width="170" Click="startOver" FontSize="16" Height="50" Background="Honeydew" BorderThickness="7" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="Order Now!" HorizontalAlignment="Left" Margin="361,596,0,0" VerticalAlignment="Top" Width="170" Click="completeOrder" FontSize="16" Height="50" Background="Honeydew" BorderThickness="7"/>

    </Grid>
</Page>
