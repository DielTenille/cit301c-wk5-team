<Page x:Class="Mega_Escritorio_Tenille_Tisha.OrderSummaryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="Order Summary">

    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="347" Margin="171,165,0,0" VerticalAlignment="Top" Width="360"/>
        <TextBox x:Name="label" Text="Your Custom Desk Quote" TextWrapping="Wrap" TextAlignment="Center" IsReadOnly="True" HorizontalAlignment="Left" Margin="100,73,0,0" VerticalAlignment="Top" Height="87" Width="504" FontSize="24" FontWeight="Bold" BorderThickness="0"/>
        <Button x:Name="button" Content="Start Over" HorizontalAlignment="Left" Margin="171,548,0,0" VerticalAlignment="Top" Width="104" Click="startOver" FontSize="16" Height="30"/>
        <Button x:Name="button1" Content="Order Now!" HorizontalAlignment="Left" Margin="423,548,0,0" VerticalAlignment="Top" Width="105" Click="completeOrder" FontSize="16" Height="30"/>

    </Grid>
</Page>
