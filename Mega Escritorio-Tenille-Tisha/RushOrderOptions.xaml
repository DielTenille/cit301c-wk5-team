<Page x:Class="Mega_Escritorio_Tenille_Tisha.RushOrderOptions"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="Rush Order Options">

    <Grid>
        <RadioButton x:Name="radioButton" Content="3 - Days" HorizontalAlignment="Left" Margin="99,112,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButton1" Content="5 - Days" HorizontalAlignment="Left" Margin="99,132,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="radioButton2" Content="7 - Days" HorizontalAlignment="Left" Margin="99,152,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label" Content="Choose Your Shipping Time:" HorizontalAlignment="Left" Margin="74,41,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button1" Content="See Quote!" HorizontalAlignment="Left" Margin="194,212,0,0" VerticalAlignment="Top" Width="75" Click="getQuote"/>
    </Grid>
</Page>
