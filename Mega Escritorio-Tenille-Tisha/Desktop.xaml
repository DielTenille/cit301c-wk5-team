<Page x:Class="Mega_Escritorio_Tenille_Tisha.Desktop"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Mega_Escritorio_Tenille_Tisha"
      mc:Ignorable="d" 
      d:DesignHeight="700" d:DesignWidth="700"
      Title="Desktop">

    <Grid>
        <Label x:Name="label2" Content="Please Enter the Dimensions for your Desk:" HorizontalAlignment="Left" Margin="33,20,0,0" VerticalAlignment="Top" />
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="149,77,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="149,126,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label" Content="Desktop Length" HorizontalAlignment="Left" Margin="44,74,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label1" Content="Desktop Width" HorizontalAlignment="Left" Margin="44,122,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Continue" HorizontalAlignment="Left" Margin="122,184,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        

    </Grid>
</Page>
